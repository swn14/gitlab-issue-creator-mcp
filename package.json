{
    "name": "gitlab-issue-creator-mcp",
    "version": "1.0.7",
    "type": "module",
    "description": "MCP server for creating GitLab issues (self-hosted compatible).",
    "repository": {
        "type": "git",
        "url": "https://github.com/swn14/gitlab-issue-creator-mcp.git"
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "gitlab",
        "gitlab-api",
        "issues",
        "issue-tracker",
        "issue-creator",
        "automation",
        "developer-tools",
        "cli",
        "node",
        "typescript"
    ],
    "main": "./dist/index.js",
    "exports": {
        ".": "./dist/index.js"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "license": "MIT",
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "prepublishOnly": "npm run build",
        "build": "tsc -p tsconfig.json",
        "start": "node dist/index.js",
        "dev": "node --enable-source-maps dist/index.js",
        "typecheck": "tsc -p tsconfig.json --noEmit"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.11.30",
        "typescript": "^5.5.4"
    }
}
